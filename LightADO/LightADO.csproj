<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <Version>5.2.4</Version>
    <Authors>ALGHABBAN</Authors>
    <Company>Quad Microsystem</Company>
